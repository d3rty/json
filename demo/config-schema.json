    {
      "title": "FlexKeys",
      "fields": [
        {
          "name": "FlexKeys.Disabled",
          "label": "Disabled",
          "type": "checkbox",
          "value": "true"
        },
        {
          "name": "FlexKeys.CaseInsensitive",
          "label": "CaseInsensitive",
          "type": "checkbox",
          "description": "Note: can cause a Yellow/Red result when a struct meets multiple keys that are considered the same. E.g. {\"key\":\"value\", \"KEY\":\"value\"} will be considered as: - Yellow if the struct field's tag strictly matches at least one candidate. - Red if it doesn't match strictly any candidate.",
          "value": "false"
        },
        {
          "name": "FlexKeys.ChameleonCase",
          "label": "ChameleonCase",
          "type": "checkbox",
          "description": "Note: like CaseInsensitive, this can result in Yellow or Red outcomes.",
          "value": "false"
        }
      ]
    }
  ]
